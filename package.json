{
  "name": "@digimuza/react-form",
  "description": "Description",
  "version": "0.0.1",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "src",
    "tests",
    "LICENSE",
    "tsconfig.json",
    "README.md"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "prettier": {
    "semi": false,
    "useTabs": true,
    "printWidth": 140,
    "tabWidth": 8,
    "singleQuote": true,
    "trailingComma": "es5",
    "parser": "typescript",
    "arrowParens": "always"
  },
  "keywords": [],
  "scripts": {
    "docs": "typedoc src/index.ts",
    "build": "rm -rf lib && tsc --build",
    "format": "prettier --write \"**/src/**/*.ts\"",
    "watch": "yarn link && tsc --watch",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "digimuza",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/builder-webpack4": "^6.5.8",
    "@storybook/manager-webpack4": "^6.5.8",
    "@storybook/react": "^6.5.8",
    "@storybook/testing-library": "^0.0.11",
    "@types/jest": "^27.5.1",
    "@types/node": "^13.13.5",
    "babel-loader": "^8.2.5",
    "git-conventional-commits": "^1.2.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15",
    "typescript": "4.5.x"
  },
  "dependencies": {
    "@types/react": "^18.0.9",
    "react": "^18.1.0",
    "react-hook-form": "^7.31.3",
    "ts-prime": "^1.0.0",
    "type-fest": "^2.13.0",
    "zod": "^3.17.3"
  }
}